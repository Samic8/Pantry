{
  "name": "pantry",
  "version": "1.0.0",
  "description": "Estimates when you need to re-stock your kitchen pantry.",
  "main": "src/index.html",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "prettier-plugin-elm": "^0.4.2",
    "prisma-client-lib": "^1.28.5"
  },
  "devDependencies": {},
  "scripts": {
