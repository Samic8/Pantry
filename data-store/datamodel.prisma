type Cupboard {
  id: ID! @unique
  title: String!
  urlSlug: String! @unique
  items: [Item]
}

type Item {
  id: ID! @unique
  cupboard: Cupboard!
  name: String!
  maxOnHand: Int!
  unit: String!
  restocks: [Restock]
}

type Restock {
  date: DateTime!
  newOnHand: Int!
  previousRestock: Restock
  userEstimateRunOut: DateTime
  didRunOut: DateTime
  leftOverFromPrevious: Int
}