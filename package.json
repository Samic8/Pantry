    "make": "elm make src/Main.elm --output=dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Samic8/Pantry.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Samic8/Pantry/issues"
  },
  "homepage": "https://github.com/Samic8/Pantry#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{elm, css, js}": [
      "npm run format",
      "git add"
    ]
  }
